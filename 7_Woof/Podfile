# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Woof' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Woof
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'

  # Pods for Facebook
  pod 'Bolts'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'

  # Pods for Google
  pod 'GoogleSignIn'

  target 'WoofTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WoofUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
